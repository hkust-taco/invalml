fun f(x) =
  fun even(x) =
    fun odd(x) =
      if x == 0 then true
      else if x == 1 then false
      else even(x - 1)
    if x == 0 then true
    else if x == 1 then false
    else odd(x - 1)
  even(x)
f(3)
