:js

fun example(x) =
  if x
    |> Math.sign
      < 0 then "invalid"
      == 0 then "null"
    |> Math.abs
      > 100 then "large"
      < 10 then "small"
      else "medium"

example(0)
//│ = "null"

example(-24)
//│ = "invalid"

:expect "large"
example(200)
//│ = "large"

:expect "small"
example(5)
//│ = "small"

:expect "medium"
example(79)
//│ = "medium"
