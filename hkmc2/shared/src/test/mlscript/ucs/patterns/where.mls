data class Pair[A, B](fst: A, snd: B)


fun orderedPair(p) = p is
  Pair(a, b) where a <= b

orderedPair(Pair(4, 5))
//│ = true

orderedPair(4)
//│ = false

orderedPair(Pair(1, 1))
//│ = true

orderedPair(Pair(2, 1))
//│ = false

fun foo(p) = p is
  Pair(a, b) where
    let c = a
    a == 0

foo(Pair(5, 5))
//│ = false

foo(Pair(0, 0))
//│ = true

foo(Pair(0, 4))
//│ = true

foo(Pair(4, 0))
//│ = false


fun bar(p) = p is
  (Pair(a, b) where a > b) | Int

bar(4)
//│ = true

bar(Pair(4, 5))
//│ = false

bar(Pair(4, 3))
//│ = true

