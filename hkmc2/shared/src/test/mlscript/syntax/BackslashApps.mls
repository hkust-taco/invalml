:js


import "../../mlscript-compile/Iter.mls"


fun inc(x) = x + 1

1 \inc()
//│ = 2

1\inc()
//│ = 2

1 \inc() \inc()
//│ = 3

1 \inc()\inc()
//│ = 3

42
  \inc()
  \inc()
//│ = 44


[1, 2, 3] \Iter.mapping(inc) \Iter.toStack()
//│ = Cons(2, Cons(3, Cons(4, Nil)))

[1, 2, 3]
  \Iter.mapping(inc)
  \Iter.toStack()
//│ = Cons(2, Cons(3, Cons(4, Nil)))

[1, 2, 3]
  \Iter.mapping @ x => x + 1
  \Iter.toStack()
//│ = Cons(2, Cons(3, Cons(4, Nil)))

[1, 2, 3]
  \Iter.mapping @
    _ + 1
  \Iter.toStack()
//│ = Cons(2, Cons(3, Cons(4, Nil)))


