:js

import "../../../mlscript-compile/apps/parsing/Test.mls"

open Test { example }

example of """
:tree
let x = 0 in
while x < 10 do
  print_int x;
  print_newline ();
  x <- x + 1
done
"""
//│ > LetIn:
//│ >   bindings = Stack of 
//│ >     Pair:
//│ >       first = Ident of "x"
//│ >       second = Literal#Integer of "0"
//│ >   body = While:
//│ >     condition = App:
//│ >       callee = Ident of "<"
//│ >       arguments = Stack of 
//│ >         Ident of "x"
//│ >         Literal#Integer of "10"
//│ >     body = Sequence of Stack of 
//│ >       App:
//│ >         callee = Ident of "print_int"
//│ >         arguments = Stack of 
//│ >           Ident of "x"
//│ >       App:
//│ >         callee = Ident of "print_newline"
//│ >         arguments = Stack of 
//│ >           Tuple of Nil
//│ >       App:
//│ >         callee = Ident of "+"
//│ >         arguments = Stack of 
//│ >           App:
//│ >             callee = Ident of "<-"
//│ >             arguments = Stack of 
//│ >               Ident of "x"
//│ >               Ident of "x"
//│ >           Literal#Integer of "1"
//│ > «let x = 0 in while x < 10 do print_int(x); print_newline(()); (x <- x) + 1 done»

example of """
:tree
for i = 0 to 10 do
  print i
done
"""
//│ > For:
//│ >   head = Ident of "i"
//│ >   start = Literal#Integer of "0"
//│ >   end = Literal#Integer of "10"
//│ >   body = App:
//│ >     callee = Ident of "print"
//│ >     arguments = Stack of 
//│ >       Ident of "i"
//│ > «for i = 0 to 10 do print(i) done»
