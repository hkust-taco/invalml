fun printParsed(...lines) =
  tracer.reset()
  let
    source = lines.join("\n")
    tokens = Lexer.lex(source)
    tree = parse(tokens)
  print of showAsTree(tree)
  
fun debugParsed(...lines) =
  set tracer.enabled = true
  let result = printParsed of ...lines
  set tracer.enabled = false
  result

