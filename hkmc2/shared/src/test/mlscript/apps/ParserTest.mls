example of """
:tree
let x: int -> int -> int = fun x y -> x + y
"""
//│ > Define:
//│ >   kind = Let(false)
//│ >   items = Stack of 
//│ >     Pair:
//│ >       first = Infix:
//│ >         op = Keyword(`:`, 4, 3)
//│ >         lhs = Ident of "x"
//│ >         rhs = Infix:
//│ >           op = Keyword(`->`, 5, 4)
//│ >           lhs = Ident of "int"
//│ >           rhs = Infix:
//│ >             op = Keyword(`->`, 5, 4)
//│ >             lhs = Ident of "int"
//│ >             rhs = Ident of "int"
//│ >       second = Lambda:
//│ >         params = Stack of 
//│ >           App:
//│ >             callee = Ident of "x"
//│ >             arguments = Stack of 
//│ >               Ident of "y"
//│ >         body = App:
//│ >           callee = Ident of "+"
//│ >           arguments = Stack of 
//│ >             Ident of "x"
//│ >             Ident of "y"
//│ > «let x : int -> int -> int = fun x(y) -> x + y»

